from Crypto.Cipher import AES
from Crypto import Random

print "Jepe mesazhin:"
message = raw_input()


print "qelesi: "
key = raw_input()

iv = Random.new().read(AES.block_size)
cipher = AES.new(key, AES.MODE_CFB, iv)

msg = iv + cipher.encrypt(message)

print iv

dec = cipher.decrypt(msg)

print "Encrypted: ",msg

print dec
